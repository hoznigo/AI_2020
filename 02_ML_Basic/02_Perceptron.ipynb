   "source": [
    "def OR(x1, x2):\n",
    "  x = np.array([x1, x2])\n",
    "  w = np.array([0.5, 0.5])\n",
    "  b = -0.2\n",
    "  tmp = np.sum(w*x) + b\n",
    "  #print(x1, x2, tmp)\n",
    "  if tmp <= 0 :\n",
    "    return 0\n",
    "  else:\n",
    "    return 1"
   ]
